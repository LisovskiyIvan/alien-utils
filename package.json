{
  "name": "@dayme/utils",
  "version": "0.1.0",
  "type": "module",
  "description": "Utility functions library",
  "keywords": [
    "utils",
    "utilities",
    "helpers"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently \"tsc --watch\" \"vite build --watch\"",
    "build": "tsc && vite build",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "playground:dev": "vite dev --config app/playground/vite.config.ts",
    "playground": "concurrently \"bun run dev\" \"bun run playground:dev\"",
    "docs:dev": "vitepress dev app/docs",
    "docs:build": "vitepress build app/docs",
    "docs:preview": "vitepress preview app/docs"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "concurrently": "^9.1.0",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-dts": "^4.3.0",
    "vitepress": "^1.6.3"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "dependencies": {
    "@types/bun": "^1.3.5"
  }
}
